#include<iostream>			//Подключаем заголовок библиотеки ввода и вывода

using namespace std;		//Объявляем что будем использовать все последующие 

индентификаторы из пространства имен std


int main()				//Точка входа в программу
{

	cout << "Hello world!\n";	//Выводим Hello world! с последующим переносом на следующую строку
	

return 0;			//В конце работы программы выводим 0
}
